{
  "name": "resource.integrations.api.files",
  "version": "1.0.0",
  "description": "Service for files management",
  "repository": "https://bitbucket.web-bee.ru/projects/RESOURCE/repos/resource.integrations.api.files/browse",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "db:create": "ts-node src/db/createDatabase.ts",
    "migration:run": "npm run typeorm migration:run -- -d dist/db/typeorm.config.js",
    "migration:generate": "npm run typeorm -- -d src/db/typeorm.config.ts migration:generate src/db/migrations/new",
    "migration:create": "npm run typeorm -- migration:create src/db/migrations/new",
    "migration:revert": "npm run typeorm -- -d src/db/typeorm.config.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@fastify/multipart": "8.3.0",
    "@fastify/static": "7.0.4",
    "@nestjs/common": "10.4.15",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/platform-fastify": "10.4.15",
    "@nestjs/schedule": "4.1.2",
    "@nestjs/swagger": "8.1.0",
    "@nestjs/typeorm": "10.0.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fastify-swagger": "5.1.1",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "minio": "8.0.2",
    "pg": "8.13.1",
    "typeorm": "0.3.20",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "22.10.1",
    "@types/pg": "8.11.10",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-no-relative-import-paths": "1.5.5",
    "eslint-plugin-prettier": "5.2.1",
    "typescript": "5.7.2"
  }
}
